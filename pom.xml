        <repository>
            <id>spring.snapshots</id>
            <name>Spring Snapshot Repository</name>
            <url>https://repo.spring.io/libs-snapshot/</url>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
            <releases>
                <enabled>false</enabled>
            </releases>
        </repository>

        <repository>
            <id>alfresco</id>
            <name>Alfresco Public Repository</name>
            <url>https://artifacts.alfresco.com/nexus/content/repositories/public/</url>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
            <releases>
                <enabled>true</enabled>
            </releases>
        </repository>

    </repositories>

    <build>
        <plugins>
            <plugin>
                <artifactId>maven-release-plugin</artifactId>
                <version>2.4.2</version>
                <dependencies>
                    <dependency>
                        <groupId>org.apache.maven.scm</groupId>
                        <artifactId>maven-scm-provider-gitexe</artifactId>
                        <version>1.9.1</version>
                    </dependency>
                    <dependency>
                        <groupId>org.apache.maven.scm</groupId>
                        <artifactId>maven-scm-api</artifactId>
                        <version>1.9.1</version>
                    </dependency>
                </dependencies>
            </plugin>
            <plugin>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.17</version>
                <configuration>
                    <excludes>
                        <exclude>**/*ManualTest*.java</exclude>
                        <exclude>**/*IntegrationTest*.java</exclude>
                        <exclude>**/PrepareTestEnvironment.java</exclude>
                    </excludes>
                </configuration>
            </plugin>
        </plugins>
    </build>


    <properties>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
        <javax.el.version>2.2.4</javax.el.version>
        <hibernate.validator.version>5.0.1.Final</hibernate.validator.version>
        <jackson-2-version>2.8.1</jackson-2-version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <jersey.version>2.23</jersey.version>
        <spring.security.version>4.2.2.RELEASE</spring.security.version>
        <httpclient.version>4.5.2</httpclient.version>
        <eclipse.jetty.version>9.2.19.v20160908</eclipse.jetty.version>
        <cxf.version>3.1.4</cxf.version>
        <hazelcast.version>2.1.2</hazelcast.version>
        <spring.version>4.3.7.RELEASE</spring.version>
        <spring.tx.version>4.3.7.RELEASE</spring.tx.version>
        <jpa.version>2.0.0</jpa.version>
        <junit.version>4.11</junit.version>
        <hibernate.version>4.3.8.Final</hibernate.version>
        <aspectj.version>1.6.12</aspectj.version>
        <slf4j.version>1.7.6</slf4j.version>
        <jodatime.version>2.9.7</jodatime.version>
        <logback.version>1.1.2</logback.version>
        <mockito.version>1.10.8</mockito.version>
        <lombok.version>1.16.16</lombok.version>
        <jjwt.version>0.6.0</jjwt.version>
    </properties>



